<div>
{{#issues}}
    <div style="display:inline-block;margin:20px;border:1px solid gray;padding:20px;">
        <h2>Queued</h2>
        {{#queued.length}}
            <ul>
            {{#queued.data}}
                <li><b>{{title}}</b></li>
            {{/queued.data}}
            </ul>
        {{/queued.length}}
    </div>
    <div style="display:inline-block;margin:20px;border:1px solid gray;padding:20px;">
        <h2>Active</h2>
        {{#active.length}}
            <ul>
            {{#active.data}}
                <li><b>{{title}}</b></li>
            {{/active.data}}
            </ul>
        {{/active.length}}
    </div>
    <div style="display:inline-block;margin:20px;border:1px solid gray;padding:20px;">
        <h2>Completed</h2>
        {{#completed.length}}
            <ul>
            {{#completed.data}}
                <li><b>{{title}}</b></li>
            {{/completed.data}}
            </ul>
        {{/completed.length}}
    </div>
{{/issues}}
</div>